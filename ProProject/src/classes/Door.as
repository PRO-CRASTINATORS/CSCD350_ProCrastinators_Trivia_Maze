package  classes{	import flash.display.MovieClip;	import com.natejc.utils.StageRef;	/**	 * ...	 * @author ...	 */	public class Door extends MovieClip	{		//private var _que		:QandA;		private var _locked 	:int;		private var _sType		:String;		private var mc			:MovieClip;				public function Door($type:String) 		{			//this._que = QuestionFactory.getQandA();			this._sType = $type;			this._locked = 0;		}		public function getMc():MovieClip		{			return this.mc;		}				public function killDoor():void		{			StageRef.stage.removeChild(this.mc);			this.mc = null;		}				public function addDoorToStage():void		{			if (this._sType == "north") 			{				this.mc = new zNorthDoor();				StageRef.stage.addChildAt(mc, 1);			}			if (this._sType == "south") 			{				this.mc = new zSouthDoor();				StageRef.stage.addChildAt(mc, 1);			}			if (this._sType == "west") 			{				this.mc = new zWestDoor();				StageRef.stage.addChildAt(mc, 1);			}			if (this._sType == "east") 			{				this.mc = new zEastDoor();				StageRef.stage.addChildAt(mc, 1);			}		}			}}