package  classes{	import flash.net.URLLoader;	import flash.display.MovieClip;	import flash.events.Event;	import classes.Room;	import classes.Door;		public class MazeGen extends MovieClip	{		private var aMap		:Array;		private var rMap		:Array;		private var mapType		:String;		private var aDimensions	:Array;				public function MazeGen() 		{			// constructor code		}				public function loadMap($loader:URLLoader):void		{			var temp:Array = $loader.data.split("\n");			var dtemp:String = temp[0];			aDimensions = dtemp.split("x");			aMap= new Array(Number(aDimensions[0]));						for(var i:int=1; i <= Number(aDimensions[1]); i++)			{				aMap[i-1] = String(temp[i]).split("|");			}			this.convertMap();		}				public function getMap():Array		{			return this.rMap;		}		private function convertMap():void		{			rMap = new Array(Number(aDimensions[0]));						//QuestionFactory fact = new QuestionFactory();			//fact.genQlist();				for (var k:int = 0; k < Number(aDimensions[0]); k++) 			{				var temp:Array = new Array(Number(aDimensions[1]));								for (var j:int = 0; j < Number(aDimensions[1]); j++) 				{					temp[j] = null;					if (aMap[k][j] == "r") 					{						temp[j] = new Room("zelda");												if (aMap[k - 1][j] == "r")							Room(temp[j]).setNorth(new Door("north"));							if (aMap[k][j + 1] == "r")							Room(temp[j]).setEast(new Door("east"));							if (aMap[k + 1][j] == "r")							Room(temp[j]).setSouth(new Door("south"));							if (aMap[k][j - 1] == "r")							Room(temp[j]).setWest(new Door("west"));					}//end if				}// end inner for				rMap[k] = temp;			}// end outer for		}	}	}